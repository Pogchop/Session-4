# Session-4
#include <stdio.h>

int main()
{
    int a;
    int b;
    int c;
    int total;
    
    printf("a = ");
    scanf("%d",&a);
    
    printf("b = ");
    scanf("%d",&b);
    
    printf("c = ");
    scanf("%d",&c);
    
    total = a + b + c;
    
    printf("total = %d",total);


    return 0;
}
